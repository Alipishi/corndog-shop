@model IEnumerable<Product>
@{
    ViewData["Title"] = ViewData["GroupName"];
}

<div class="text-center">
    <h1 class="display-4"> @ViewData["GroupName"] </h1>
     <hr class="border border-dark border-1 opacity-25" />



    <div class="row">
        @foreach (var item in Model)
        {
            <partial name="_BoxProduct" model="@item" />
        }
    </div>
</div>

